{
  "debug": true,
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "master",
    {
      "name": "beta",
      "prerelease": true
    }
  ],
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "angular",
        "releaseRules": "../scripts/release-rules.cjs"
      }
    ],
    "@semantic-release/git"
  ],
  "tagFormat": "redis/v${version}"
}