{
    "debug": true,
    "branches": [
        "+([0-9])?(.{+([0-9]),x}).x",
        "master",
        {
            "name": "beta",
            "prerelease": true
        }
    ],
    "plugins": [
        [
            "@semantic-release/exec",
            {
                "analyzeCommitsCmd": "./scripts/analyze-commits.sh ${commits} ${cwd}"
            }
        ],
        "@semantic-release/release-notes-generator",
        "@semantic-release/github"
    ]
}